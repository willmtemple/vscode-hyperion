		"tab.activeBorder": "#1a3d4c",
		"tab.border": "#3a5d6c",
		"tab.inactiveForeground": "#aeccd8",
		"tab.activeForeground": "#9ebcc8",
		// Scrollbars
		"scrollbarSlider.activeBackground": "#ffffff40",
		"scrollbarSlider.hoverBackground": "#ffffff30",
		"scrollbarSlider.background": "#ffffff20",
		// List
		"list.hoverBackground": "#2a4b5c",
		"list.activeSelectionBackground": "#1d8fde",
		"list.focusBackground": "#0a2d3c",
		"list.highlightForeground": "#ffff00",
		"list.inactiveFocusBackground": "#0d7fce",
