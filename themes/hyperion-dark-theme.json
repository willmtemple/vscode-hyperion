		"activityBar.background": "#3a5b6c",
		"activityBarBadge.background": "#1d8fde",
		// Widgets
		"button.background": "#1d8fde",
		"dropdown.background": "#1a3d4c",
		"notifications.background": "#1a3d4c",
		"input.background": "#3a5d6c",
		"badge.background": "#0a2d3c",
		"badge.foreground": "#9eccd8",
