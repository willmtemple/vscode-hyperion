				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Object Variables",
			"scope": [
				"variable.other.object"
			],
			"settings": {
				"foreground": "#dc76aa",
			}
